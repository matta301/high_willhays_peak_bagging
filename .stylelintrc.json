{
    "extends": ["stylelint-config-sass-guidelines", "stylelint-config-airbnb"],
    "plugins": ["stylelint-order"],
    "rules": {
      "indentation": 2,
      "property-no-unknown": true,
      "color-no-invalid-hex": true,
      "block-no-empty": true,
      "order/properties-alphabetical-order": true
    }
  }
  